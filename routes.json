{
    "routes": [
      {
        "route": "/",
        "serve": "/src/index.html",
        "statusCode": 200
      },
      {
        "route": "/dashboard",
        "serve": "/src/financialTracking.html",
        "statusCode": 200
      },
      {
        "route": "/accounts",
        "serve": "/src/accounts.html",
        "statusCode": 200
      },
      {
        "route": "/login",
        "serve": "/src/loggedIn.html",
